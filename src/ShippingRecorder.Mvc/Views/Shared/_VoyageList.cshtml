@using ShippingRecorder.Entities.Db
@model IEnumerable<Voyage>

<select id="VoyageList" name="VoyageList" size="8" class="form-control">
    @if (Model?.Count() > 0)
    {
        foreach (var voyage in Model)
        {
            <option value="@voyage.Id">@voyage.ToString()</option>
        }
    }
</select>
